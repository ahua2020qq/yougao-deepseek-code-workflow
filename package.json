{
    "name": "yougao-deepseek-code-workflow",
    "displayName": "油糕出品 | DeepSeek Code Workflow",
    "description": "油糕出品 | 可视化AI编程Agent编排工具 | 复刻Claude Code Workflow能力，适配火山引擎DeepSeek，让编码更有烟火气",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.80.0"
    },
    "categories": [
        "Programming Languages",
        "AI"
    ],
    "activationEvents": [
        "onStartupFinished"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "yougao-deepseek-code-workflow.runAgent",
                "title": "油糕出品 | 运行DeepSeek Code Agent",
                "category": "油糕出品"
            }
        ],
        "configuration": {
            "title": "DeepSeek Code Workflow",
            "properties": {
                "yougao-deepseek-code-workflow.apiKey": {
                    "type": "string",
                    "description": "火山引擎API Key",
                    "default": ""
                },
                "yougao-deepseek-code-workflow.baseUrl": {
                    "type": "string",
                    "default": "https://ark.cn-beijing.volces.com/api/v3",
                    "description": "火山引擎DeepSeek API Base URL"
                },
                "yougao-deepseek-code-workflow.model": {
                    "type": "string",
                    "enum": [
                        "deepseek-v3-2-251201",
                        "deepseek-chat",
                        "deepseek-reasoner"
                    ],
                    "default": "deepseek-v3-2-251201",
                    "description": "DeepSeek模型选择"
                }
            }
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "package": "vsce package --no-dependencies",
        "publish": "vsce publish"
    },
    "keywords": [
        "油糕yougao",
        "AI编程",
        "VSCode插件",
        "DeepSeek",
        "火山引擎",
        "可视化Agent"
    ],
    "author": "ahua2020qq",
    "license": "MIT",
    "publisher": "yougao-dev",
    "repository": {
        "type": "git",
        "url": "https://github.com/ahua2020qq/yougao-deepseek-code-workflow.git"
    },
    "devDependencies": {
        "@types/node": "^20.5.0",
        "@types/vscode": "^1.80.0",
        "typescript": "^5.1.0",
        "vsce": "^2.15.0"
    },
    "dependencies": {
        "openai": "^4.104.0"
    }
}
